<?xml version="1.0" encoding="utf-8"?>
<!-- 
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

	Configuration file for UrlRewriteFilter
	http://tuckey.org/urlrewrite/
-->
<urlrewrite>
	<rule>
		<from>/sso/(.*)/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!sso.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4&amp;sso_token=$5</to>
	</rule>
	<rule>
		<from>/sysuser/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!sysuser.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/syslog/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!syslog.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/sysnotify/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!sysnotify.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/sysalarm/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!sysalarm.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/sysemail/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!sysemail.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/programpublish/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!programpublish.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/synchfiles/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!synchfiles.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/reportmonitor/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!reportmonitor.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/configmonitor/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!configmonitor.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/diggapi</from>
		<to>/digg!api.action</to>
	</rule>
	<rule>
		<from>/diggexport</from>
		<to>/digg!apiexport.action</to>
	</rule>
	<rule>
		<from>/digg/opendata</from>
		<to>/digg!opendata.action</to>
	</rule>
	<rule>
		<from>/digg/open</from>
		<to>/digg!open.action</to>
	</rule>
	<rule>
		<from>/securitycallback/(.*)</from>
		<to>/security!callback.action?script=$1</to>
	</rule>
	<rule>
		<from>/addsysuser/(.*)/(.*)/(.*)/(.*)</from>
		<to>/security!addsysuser.action?account=$1&amp;ts=$2&amp;nonce=$3&amp;signature=$4</to>
	</rule>
	<rule>
		<from>/d/(.*)</from>
		<to>/digg!query.action?gridxml=$1</to>
	</rule>
	<rule>
		<from>/p/(.*)</from>
		<to>/http!doGet.action?link=$1</to>
	</rule>
	<rule>
		<from>/z/(.*)</from>
		<to>/http!image.action?link=$1</to>
	</rule>
	<rule>
		<from>/i/(.*)</from>
		<to>/http!imagezoom.action?link=$1</to>
	</rule>
	<rule>
		<from>/modulelogo/(.*)</from>
		<to>/modules!logo.action?sysid=$1</to>
	</rule>
	<rule>
		<from>/zkcfg/(.*)</from>
		<to>/dev!preset.action?gridxml=$1</to>
	</rule>
	<rule>
		<from>/zoom/([0-9]+)/(.*)</from>
		<to>/http!image.action?scale=$1&amp;link=$2</to>
	</rule>
	<rule>
		<from>/main</from>
		<to>/login!main.action</to>
	</rule>
	<rule>
		<from>/index</from>
		<to>/login!index.action</to>
	</rule>
	<rule>
		<from>/logon</from>
		<to>/login!doSignin.action</to>
	</rule>
	<rule>
		<from>/fogotpassowrd</from>
		<to>/login!fogotpassowrd.action</to>
	</rule>
	<rule>
		<from>^/intro$</from>
		<to>/about/intro.html</to>
	</rule>
	<rule>
		<from>^/privacy$</from>
		<to>/about/privacy.html</to>
	</rule>
	<rule>
		<from>^/sessionout$</from>
		<to>/skit/jsp/sessionout.jsp</to>
	</rule>
	<rule>
		<from>/sql/(.*)</from>
		<to>/helper!sqlquery.action?db=$1</to>
	</rule>
	<rule>
		<from>/checkin</from>
		<to>/cos!checkin.action</to>
	</rule>
	<rule>
		<from>^/404$</from>
		<to>/skit/jsp/404.jsp</to>
	</rule>
	<rule>
		<from>^/403$</from>
		<to>/skit/jsp/403.jsp</to>
	</rule>
	<rule>
		<from>^/500$</from>
		<to>/skit/jsp/500.jsp</to>
	</rule>
 </urlrewrite>
